<h2>Pedidos</h2>
<table>
    <tbody>
        <tr>
            <td>ID</td>
            <td>ID_MSG</td>
            <td>MAIL</td>
            <td></td>
        </tr>
        <% for(var i=0;i<datos.length;i++){ %>
        <tr>
            <td><%= datos[i].id %></td>
            <td><%= datos[i].id_correo %></td>
            <td><%= datos[i].email %></td>
            <td ><img src="/img/vista.png" onclick="AbrirCSV(<%= datos[i].id %>)" /></td>
        </tr>
        <% } %>
    </tbody>
</table>
